<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AlarmClockLoadingView">
        <attr name="timeForSpeed" format="integer|reference" />
        <attr name="progress" format="integer|reference" />
        <attr name="progressTextSize" format="dimension|reference" />
        <attr name="progressTextColor" format="color|reference" />
        <attr name="progressDescription" format="string|reference" />
        <attr name="desTextSize" format="dimension|reference" />
        <attr name="desTextColor" format="color|reference" />
