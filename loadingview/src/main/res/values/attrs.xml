    <declare-styleable name="HappyFatDogeLoadingView">
        <attr name="withProgressDisplay" format="boolean" />
        <attr name="fatDogeProgress" format="integer|reference" />
        <attr name="fatDogeProgressTextSize" format="dimension|reference" />
        <attr name="fatDogeProgressTextColor" format="color|reference" />
        <attr name="fatDogeProgressDescription" format="string|reference" />
        <attr name="fatDogeDesTextSize" format="dimension|reference" />
        <attr name="fatDogeDesTextColor" format="color|reference" />
    </declare-styleable>
</resources>